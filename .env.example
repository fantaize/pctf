# ── Required ──
CLAUDE_CODE_OAUTH_TOKEN=sk-ant-oat01-...   # Run 'claude setup-token' to generate
PICOCTF_USERNAME=your_username
PICOCTF_PASSWORD=your_password

# ── Optional ──
PICOCTF_URL=https://play.picoctf.org
MODEL=claude-opus-4-6
MAX_ATTEMPTS_PER_CHALLENGE=3
CATEGORIES=               # comma-separated: crypto,forensics,rev,pwn,web,misc,general
MAX_POINTS=500             # skip challenges above this point value
DRY_RUN=false              # solve but don't submit flags
LOG_LEVEL=INFO
